# ===========================================
# TicketRush Environment Configuration
# ===========================================
# Copy this file to .env and fill in the values

# PostgreSQL
POSTGRES_USER=ticketrush
POSTGRES_PASSWORD=ticketrush_secret
POSTGRES_DB=ticketrush_db
DATABASE_URL="postgresql://ticketrush:ticketrush_secret@localhost:5432/ticketrush_db?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# RabbitMQ
RABBITMQ_USER=ticketrush
RABBITMQ_PASSWORD=ticketrush_secret
RABBITMQ_URL="amqp://ticketrush:ticketrush_secret@localhost:5672"

# Next.js
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV=development
